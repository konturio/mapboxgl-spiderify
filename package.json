{
  "name": "mapboxgl-spiderify",
  "version": "1.0.0",
  "description": "",
  "sideEffects": false,
  "types": "./lib/types/index.d.ts",
  "browser": "./lib/esm/index.js",
  "main": "./lib/umd/index.js",
  "module": "./lib/esm/index.js",
  "scripts": {
    "build": "npm run build:types && npm run build:esm && npm run build:umd && npm run rollup",
    "build:clean": "rm -rf lib",
    "build:types": "tsc -p tsconfig.types.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:umd": "tsc -p tsconfig.umd.json",
    "postbuild": "cpy style.css lib/",
    "rollup:alt": "rollup lib/esm/index.js --file lib/bundle.js --format iife --name MyLibrary",
    "rollup": "rollup lib/esm/index.js --file lib/bundle.all.js --format umd --name MyLibrary"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konturio/mapboxgl-spiderify.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/konturio/mapboxgl-spiderify/issues"
  },
  "homepage": "https://github.com/konturio/mapboxgl-spiderify#readme",
  "devDependencies": {
    "@types/mapbox-gl": "^1.12.2",
    "cpy-cli": "^3.1.1",
    "rollup": "^2.26.11",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "mapbox-gl": "^1.12.0"
  }
}
