  },
  "homepage": "https://github.com/zbarbuto/typescript-library-starter#readme",
  "devDependencies": {
    "rollup": "^2.26.11",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "date-fns": "^2.16.1"
  }
